
<div class="row" id="rowArea">
<ul id="show">

</ul>
</div>
<script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
<script type="text/javascript">
   
$(window).load(function(){
	$.getJSON("http://spajammadobenwebapi.azurewebsites.net/api/Voices", function(json){
  		var show = "";
  		json.forEach(function(value){
  			show += "<li>" + "<img src='~/Scripts/1.jpeg'>" + value.VoiceAnalysisResult + "</li>";
  		});
  		$("#show").html(show);
	});
	
});

</script>

